triggers='CREATE DEFINER=`redha`@`%` TRIGGER `lab_device_AFTER_INSERT` AFTER INSERT ON `lab_device` FOR EACH ROW BEGIN\n-- sync\n  	DECLARE original_query TEXT;\n    SET original_query = (SELECT info FROM INFORMATION_SCHEMA.PROCESSLIST WHERE id = CONNECTION_ID());\n    INSERT INTO `offline_sync`(`table_name`,`operation`,`lab_id`,`query`) VALUES (\'lab_device\',\'insert\',new.lab_id,original_query);\n  --\nEND' 'CREATE DEFINER=`redha`@`%` TRIGGER `lab_device_AFTER_UPDATE` AFTER UPDATE ON `lab_device` FOR EACH ROW BEGIN\n-- sync\n  	DECLARE original_query TEXT;\n    SET original_query = (SELECT info FROM INFORMATION_SCHEMA.PROCESSLIST WHERE id = CONNECTION_ID());\n    INSERT INTO `offline_sync`(`table_name`,`operation`,`lab_id`,`query`) VALUES (\'lab_device\',\'update\',new.lab_id,original_query);\n  --\nEND' 'CREATE DEFINER=`redha`@`%` TRIGGER `lab_device_AFTER_DELETE` AFTER DELETE ON `lab_device` FOR EACH ROW BEGIN\n -- sync\n  	DECLARE original_query TEXT;\n    SET original_query = (SELECT info FROM INFORMATION_SCHEMA.PROCESSLIST WHERE id = CONNECTION_ID());\n    INSERT INTO `offline_sync`(`table_name`,`operation`,`lab_id`,`query`) VALUES (\'lab_device\',\'del\',old.lab_id,original_query);\n  --\nEND'
